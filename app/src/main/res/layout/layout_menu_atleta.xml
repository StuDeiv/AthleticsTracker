<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondomenuatleta"
    tools:context=".MenuAtleta">

    <Button
        android:id="@+id/buttonRegistrosPersonales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/registros_personales"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnVerClubAtleta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ver_club"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonRegistrosPersonales" />

    <Button
        android:id="@+id/btnComparador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="comparador"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnVerClubAtleta" />
</androidx.constraintlayout.widget.ConstraintLayout>